{
  "name": "neurolov-network",
  "private": true,
  "version": "1.0.0",
  "description": "Browser-first decentralized AI compute platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:edge": "node dist/edge/server.js",
    "build:contracts": "cd contracts/programs/neurolov && cargo build-bpf",
    "deploy:contracts": "solana program deploy target/deploy/neurolov.so",
    "initialize-program": "node scripts/initialize-program.js",
    "monitor": "node scripts/monitor.js",
    "deploy:cloudflare": "wrangler publish",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start:all": "node scripts/dev.js",
    "start:swarm": "node src/services/server/swarmServer.js",
    "start:compute": "node src/services/server/computeServer.js",
    "start:task": "node src/services/server/taskServer.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.1.1",
    "@noble/ed25519": "^2.2.3",
    "@project-serum/anchor": "^0.26.0",
    "@solana/wallet-adapter-backpack": "^0.1.14",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-phantom": "^0.9.24",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-adapter-solflare": "^0.6.28",
    "@solana/wallet-adapter-wallets": "^0.19.24",
    "@solana/web3.js": "^1.87.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "recoil": "^0.7.7",
    "redis": "^4.6.10",
    "styled-components": "^6.1.14",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "wrangler": "^3.15.0"
  }
}
